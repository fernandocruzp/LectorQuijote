import java.util.HashMap;
import mx.ciencias.ArbolRojinegro;
public class ImpresionFinal implements AccionVerticeArbolBinario<T> {
    private HashMap<String,Integer> dicc;
    public ImpresionFinal(HashMap<String,Integer> dicc){
	this.dicc=dicc;
    }
    @Override
    public void actua(VerticeArbolBinario<T> vertice){
	System.out.println(vertice.get() + ": " + dicc.get(vertice.get()));
    }

}
